        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartAgriculture">
        <activity android:name=".wx.other.PlanDetailsActivity"></activity>
        <activity
            android:name=".wx.ui.LoginActivityPlus"
            android:launchMode="singleTask"></activity>
        <activity android:name=".wx.other.InspectionDetailsActivity" />
        <activity android:name=".wx.ui.RegisteredActivity" />
        <activity android:name=".gyx.GYXActivity"></activity>
        <activity android:name=".wx.other.WritePlanActivity" />
        <activity android:name=".wx.ui.StartUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".wx.ui.GuideActivity" />
        <activity android:name=".wx.ui.LoginActivity" />
        <activity android:name=".wx.MainActivity" />
        <activity android:name=".wx.test.TestActivity" />

        <provider
            android:name="cn.bmob.v3.util.BmobContentProvider"
            android:authorities="com.tfu.smartagriculture.BmobContentProvider" />
    </application>

</manifest>